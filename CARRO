CRIAÇÃO CLASS CARRO
package ObjetoCarro;

public class Carro {

    private String marca;
    private String modelo;
    private int ano;
    private String cor;

    public Carro(String marca, String modelo, int ano, String cor) {
        this.marca = marca;
        this.modelo = modelo;
        this.ano = ano;
        this.cor = cor;
    }

    public void setMarca(String marca){
        this.marca = marca;
    }
    public void setModelo(String modelo){
        this.modelo = modelo;
    }
    public void setAno(int ano){
        this.ano = ano;
    }
    public void setCor(String cor){
        this.ano = ano;
    }

    public String getMarca() {
        return marca;
    }
    public String getModelo(){
        return modelo;
    }
    public int getAno() {
        return ano;
    }
    public String getCor() {
        return cor;
    }
}

CRIAÇÃO E UTILIZAÇÃO OBJETO CARRO

import ObjetoCarro.Carro;

public class Atividade1 {
    
        public static void main(String[] args) {

            Carro ford = new Carro("Ford", "Mustang", 2018, "Preto");
            
            System.out.println(ford.getMarca());
            System.out.println(ford.getModelo());
            System.out.println(ford.getAno());
            System.out.println(ford.getCor());
        }
}
